{
  "name": "receipt-ocr",
  "version": "1.0.0",
  "description": "A TypeScript library for extracting structured product data from receipt images using multimodal LLMs",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "receipt",
    "ocr",
    "gemini",
    "llm",
    "multimodal",
    "typescript"
  ],
  "author": "Beinan Hu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cosplit-now/receipt-ocr.git"
  },
  "homepage": "https://github.com/cosplit-now/receipt-ocr#readme",
  "bugs": {
    "url": "https://github.com/cosplit-now/receipt-ocr/issues"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
